= form_for @gpslog do |f|
  - if @gpslog.errors.any?
    #error_explanation
      %h2= "#{pluralize(@gpslog.errors.count, "error")} prohibited this gpslog from being saved:"
      %ul
        - @gpslog.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :userhash
    = f.text_field :userhash
  .field
    = f.label :longitude
    = f.text_field :longitude
  .field
    = f.label :latitude
    = f.text_field :latitude
  .actions
    = f.submit 'Save'
